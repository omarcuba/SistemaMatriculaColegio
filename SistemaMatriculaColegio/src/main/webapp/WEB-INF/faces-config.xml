<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">


	<navigation-rule>

		<navigation-case>
			<from-outcome>INICIO</from-outcome>
			<to-view-id>/inicio.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>LOGIN</from-outcome>
			<to-view-id>/pages/login.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>MATRICULA</from-outcome>
			<to-view-id>/pages/matricula.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>DOCENTE</from-outcome>
			<to-view-id>/pages/docente.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>ADMINISTRADOR</from-outcome>
			<to-view-id>/pages/administrador.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>CONSULTAS</from-outcome>
			<to-view-id>/pages/consultas.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>REGISTRAR_USUARIO</from-outcome>
			<to-view-id>/pages/registrarUsuario.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>RECUPERAR_CUENTA</from-outcome>
			<to-view-id>/pages/recuperarCuenta.xhtml</to-view-id>
		</navigation-case>


	</navigation-rule>

	<!-- Inicio - Necesario para la integracion Spring - JSf -->
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	<!-- Fin - Necesario para la integracion Spring - JSf -->

</faces-config>
