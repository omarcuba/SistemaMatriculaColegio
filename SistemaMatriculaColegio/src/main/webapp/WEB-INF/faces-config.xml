<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<navigation-rule>

		<navigation-case>
			<from-outcome>INICIO</from-outcome>
			<to-view-id>/inicio.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>LOGIN</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>


		<navigation-case>
			<from-outcome>INTRANET</from-outcome>
			<to-view-id>/pages/intranet.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>REGISTRAR_USUARIO</from-outcome>
			<to-view-id>/pages/registrarUsuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>RECUPERAR_CUENTA</from-outcome>
			<to-view-id>/pages/recuperarCuenta.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
